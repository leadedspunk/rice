{
    "pyprland": {
      "plugins": [
        "scratchpads",
        "workspaces_follow_focus"
      ]
    },
    "scratchpads": {
      "term": {
        "command": "kitty --class kitty-dropterm",
        "animation": "fromTop",
        "unfocus": "hide"
      },
      "volume": {
        "command": "pavucontrol",
        "unfocus": "hide",
        "animation": "fromRight"
      },
      "spotify": {
        "command": "spotify",
        "unfocus": "hide",
        "animation": "fromBottom"
      }
    }
  }